<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.ProductPanel"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Panel
		headerText="{i18n>productPanelTitle}"
		class="sapUiResponsiveMargin"
		width="auto">
		<content>
			<FormattedText
				htmlText="{invoice>ProductName}"
				class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
		</content>
	</Panel>
</mvc:View>
